{
  "name": "upsdma",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "mkdir -p build && ./node_modules/.bin/babel src --out-dir build --extensions \".js,.mjs\"",
    "serve": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "forever-build": "npm run build",
    "forever-start": "npm run build && forever start -c 'node' build/index.js",
    "forever-stop": "forever stopall",
    "forever-restart": "npm run build && forever restartall",
    "forever-restartall": "forever restartall",
    "forever-list": "forever list"
  },
